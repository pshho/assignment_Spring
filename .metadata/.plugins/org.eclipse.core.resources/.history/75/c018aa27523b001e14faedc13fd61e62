package clothing.clothesParts;

public class ClotheBodyBottom extends Clothes {
	String kinds = "하의류";

	public ClotheBodyBottom() {
		super();
	}

	public ClotheBodyBottom(String name, int status) {
		super(name, status);
	}

	@Override
	public String toString() {
		return "ClotheBodyBottom [kinds=" + kinds + ", name=" + name + ", status=" + status + "]";
	}
}
