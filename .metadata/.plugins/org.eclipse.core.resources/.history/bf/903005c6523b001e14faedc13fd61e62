package clothing.personParts;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Component;

import clothing.clothesParts.ClotheBodyBottom;
import clothing.clothesParts.ClotheBodyTop;
import clothing.clothesParts.SuitClothe;
import jakarta.annotation.Resource;

@Component
public class Body {
	String name = "몸";
	@Autowired
	ClotheBodyTop cbt; // 상의
	@Autowired
	ClotheBodyBottom cbb; // 하의
	@Autowired
	SuitClothe sc; // 한벌옷
	
	@Override
	public String toString() {
		return "Body [name=" + name + ", cbt=" + cbt + ", cbb=" + cbb + ", sc=" + sc + "]";
	}
}
