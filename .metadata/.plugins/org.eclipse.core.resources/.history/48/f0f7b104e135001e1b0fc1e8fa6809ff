package franchise;

import java.util.Arrays;

import classifications.SellSet;
import inter.InterBrand;
import menu.BeverageMenu;
import menu.HamburgerMenu;
import menu.SideMenu;

public class McBrands implements InterBrand {
	String name = "McDonald";
	SellSet[] seDis;
	HamburgerMenu hb;
	SideMenu su;
	BeverageMenu bm;

	public void setSeDis(SellSet[] seDis) {
		this.seDis = seDis;
	}
