		this.hb = hb;
	}
	public void setSu(SideMenu su) {
		this.su = su;
	}
	public void setBm(BeverageMenu bm) {
		this.bm = bm;
	}
	@Override
	public String toString() {
		return "McBrands [name=" + name + ", seDis=" + Arrays.toString(seDis) + ", hb=" + hb + ", su=" + su + ", bm="
				+ bm + "]";
	}
