package persons_p.personParts;

import clothes_p.clothesParts.ClotheBodyBottom;
import clothes_p.clothesParts.ClotheBodyTop;

// 몸
public class BodyBottom {
	String name = "몸";
	ClotheBodyTop cbt; // 상의
	ClotheBodyBottom cbb; // 하의
	
	public BodyBottom() {
		super();
	}

	public BodyBottom(ClotheBodyTop cbt, ClotheBodyBottom cbb) {
		super();
		this.cbt = cbt;
		this.cbb = cbb;
	}

	@Override
	public String toString() {
		return "Body [name=" + name + ", cbt=" + cbt + ", cbb=" + cbb + "]";
	}
}
