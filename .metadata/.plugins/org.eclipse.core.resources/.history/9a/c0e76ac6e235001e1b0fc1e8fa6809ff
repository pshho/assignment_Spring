package menu;

public class HamburgerMenu {
	String name, foodIngre, kind;
	int price, amount, size, calorie;
	
	public void setName(String name) {
		this.name = name;
	}
	public void setFoodIngre(String foodIngre) {
		this.foodIngre = foodIngre;
	}
	public void setPrice(int price) {
		this.price = price;
	}
	public void setAmount(int amount) {
		this.amount = amount;
	}
	public void setSize(int size) {
		this.size = size;
	}
	public void setCalorie(int calorie) {
		this.calorie = calorie;
	}
	public void setKind(String kind) {
		this.kind = kind;
	}
	@Override
	public String toString() {
		return "HamburgerMenu [name=" + name + ", foodIngre=" + foodIngre + ", price=" + price + ", amount=" + amount
				+ ", size=" + size + ", calorie=" + calorie + "]";
	}
}
