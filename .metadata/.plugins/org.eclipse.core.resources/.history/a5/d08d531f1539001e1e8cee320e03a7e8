package bar.store;

import java.util.List;
import java.util.Map;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.beans.factory.annotation.Qualifier;

import bar.menu.Alcohol;
import bar.menu.DrinkDish;
import jakarta.annotation.Resource;

public class Table {
	String tableName; // 가게의 몇 번 테이블
	int tableSum; // 그 테이블의 전체 가격
	
	@Autowired
	@Qualifier("drinkOrders")
	List<Map<Alcohol, Integer>> drinkOrders; // 주문한 술과 개수
	
	@Resource
	List<Map<DrinkDish, Integer>> dishOrders; // 주문한 안주와 개수

}
