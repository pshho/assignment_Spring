package menu;

public class SideMenu {
	String name, foodIngre, kind;
	int price, amount, calorie;
	
	public void setName(String name) {
		this.name = name;
	}
	public void setFoodIngre(String foodIngre) {
		this.foodIngre = foodIngre;
	}
	public void setKind(String kind) {
		this.kind = kind;
	}
	public void setPrice(int price) {
		this.price = price;
	}
	public void setAmount(int amount) {
		this.amount = amount;
	}
	public void setCalorie(int calorie) {
		this.calorie = calorie;
	}
	@Override
	public String toString() {
		return "SideMenu [name=" + name + ", foodIngre=" + foodIngre + ", kind=" + kind + ", price=" + price
				+ ", amount=" + amount + ", calorie=" + calorie + "]";
	}
}
