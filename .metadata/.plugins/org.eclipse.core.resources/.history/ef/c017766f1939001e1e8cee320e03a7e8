package bar.store;

import java.util.List;
import java.util.Map;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.beans.factory.annotation.Qualifier;

import bar.menu.Alcohol;
import bar.menu.DrinkDish;
import jakarta.annotation.Resource;

public class Table {
	String tableName; // 가게의 몇 번 테이블
	int tableSum; // 그 테이블의 전체 가격
	List<Map<Alcohol, Integer>> drinkOrders; // 주문한 술과 개수
	List<Map<DrinkDish, Integer>> dishOrders; // 주문한 안주와 개수

	public String getTableName() {
		return tableName;
	}
	public void setTableName(String tableName) {
		this.tableName = tableName;
	}
	public int getTableSum() {
		return tableSum;
	}
	public void setTableSum(int tableSum) {
		this.tableSum = tableSum;
	}
	public List<Map<Alcohol, Integer>> getDrinkOrders() {
		return drinkOrders;
	}
	public void setDrinkOrders(List<Map<Alcohol, Integer>> drinkOrders) {
		this.drinkOrders = drinkOrders;
	}
	public List<Map<DrinkDish, Integer>> getDishOrders() {
		return dishOrders;
	}
	public void setDishOrders(List<Map<DrinkDish, Integer>> dishOrders) {
		this.dishOrders = dishOrders;
	}
	@Override
	public String toString() {
		return "Table [tableName=" + tableName + ", tableSum=" + tableSum + ", drinkOrders=" + drinkOrders
				+ ", dishOrders=" + dishOrders + "]";
	}
}
