package clothes_p.clothesParts;

// 신발
public class ClotheFoots extends Clothes {
	String kinds = "신발류";
	String places; // 장소에 따라 다른 신발

	public ClotheFoots() {
		super();
	}

	public ClotheFoots(String places, String category, String name, int status) {
		super(category, name, status);
		this.places = places;
	}

	public String getPlaces() {
		return places;
	}

	@Override
	public String toString() {
		return "ClotheFoots [kinds=" + kinds + ", places=" + places + ", category=" + category + ", name=" + name
				+ ", status=" + status + "]";
	}
}
