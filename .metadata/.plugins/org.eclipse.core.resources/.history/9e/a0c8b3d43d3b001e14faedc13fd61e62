package clothing;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Component;

import clothing.personParts.Body;
import clothing.personParts.Foots;
import clothing.personParts.Hands;
import clothing.personParts.Head;
import jakarta.annotation.Resource;

@Component
public class Person {
	@Autowired
	Head hd; // 머리
	@Resource
	Body bb; // 몸
	@Autowired
	Hands hns; // 손
	@Resource
	Foots fots; // 발
	
	@Override
	public String toString() {
		return "Person [hd=" + hd + ", bb=" + bb + ", hns=" + hns + ", fots=" + fots + "]";
	}
}
