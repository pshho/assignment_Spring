package franchise;

import java.util.Arrays;

import classifications.SellSet;
import inter.InterBrand;
import menu.BeverageMenu;
import menu.HamburgerMenu;
import menu.SideMenu;

public class BurgerkingBrands implements InterBrand {
	String name = "BurgerKing";
	SellSet[] seDis;
	HamburgerMenu hb;
	SideMenu su;
	BeverageMenu bm;
	int count, sum, discount;
	
	public void setName(String name) {
		this.name = name;
	}
	public void setSeDis(SellSet[] seDis) {
		this.seDis = seDis;
	}
	public void setHb(HamburgerMenu hb) {
		this.hb = hb;
	}
	public void setSu(SideMenu su) {
		this.su = su;
	}
	public void setBm(BeverageMenu bm) {
		this.bm = bm;
	}
	public void setCount(int count) {
		this.count = count;
	}
	public void setSum(int sum) {
		this.sum = sum;
	}
	public void setDiscount(int discount) {
		this.discount = discount;
	}
	@Override
	public String toString() {
		return "BurgerkingBrands [name=" + name + ", seDis=" + Arrays.toString(seDis) + ", hb=" + hb + ", su=" + su
				+ ", bm=" + bm + ", count=" + count + ", sum=" + sum + ", discount=" + discount + "]";
	}
	@Override
	public void make() {
		// TODO Auto-generated method stub
		
	}
	@Override
	public void sell() {
		// TODO Auto-generated method stub
		
	}
	@Override
	public void calc() {
		// TODO Auto-generated method stub
		
	}
}
