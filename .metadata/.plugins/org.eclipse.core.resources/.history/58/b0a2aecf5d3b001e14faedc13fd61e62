package clothes_p.clothesParts;

// 하의
public class ClotheBodyBottom extends Clothes {
	String kinds = "하의류";
	String places;

	public ClotheBodyBottom() {
		super();
	}

	public ClotheBodyBottom(String places, String category, String name, int status) {
		super(category, name, status);
		this.places = places;
	}

	@Override
	public String toString() {
		return "ClotheBodyBottom [kinds=" + kinds + ", places=" + places + ", category=" + category + ", name=" + name
				+ ", status=" + status + "]";
	}
}
