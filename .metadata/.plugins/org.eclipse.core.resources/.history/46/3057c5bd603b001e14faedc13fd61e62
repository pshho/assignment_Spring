package persons_p.personParts;

import clothes_p.clothesParts.ClotheGlasses;
import clothes_p.clothesParts.ClotheHead;

// 머리
public class Head {
	String name = "머리";
	ClotheHead ch; // 모자류
	ClotheGlasses cg;
	
	public Head() {
		super();
	}

	public Head(ClotheHead ch, ClotheGlasses cg) {
		super();
		this.name = name;
		this.ch = ch;
		this.cg = cg;
	}

	@Override
	public String toString() {
		return "Head [name=" + name + ", ch=" + ch + "]";
	}
}
