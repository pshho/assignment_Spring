package clothing;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Component;

import clothing.personParts.Body;
import clothing.personParts.Foots;
import clothing.personParts.Hands;
import clothing.personParts.Head;
import jakarta.annotation.Resource;

@Component
public class Person {
	@Autowired
	Head hd; // 머리
	@Resource
	Body bb; // 몸
	@Autowired
	Hands hns; // 손
	@Resource
	Foots fots; // 발
	
	public Head getHd() {
		return hd;
	}
	public void setHd(Head hd) {
		this.hd = hd;
	}
	public Body getBb() {
		return bb;
	}
	public void setBb(Body bb) {
		this.bb = bb;
	}
	public Hands getHns() {
		return hns;
	}
	public void setHns(Hands hns) {
		this.hns = hns;
	}
	public Foots getFots() {
		return fots;
	}
	public void setFots(Foots fots) {
		this.fots = fots;
	}
	@Override
	public String toString() {
		return "Person [hd=" + hd + ", bb=" + bb + ", hns=" + hns + ", fots=" + fots + "]";
	}
}
