package clothing.personParts;

import org.springframework.stereotype.Component;

import clothing.Bag;
import clothing.PersonClothes;
import clothing.clothesParts.ClotheHead;

@Component
public class Head implements PersonClothes {
	String name = "머리";
	ClotheHead ch; // 모자류
	
	public Head() {
		super();
	}
	public Head(String name, ClotheHead ch) {
		super();
		this.name = name;
		this.ch = ch;
	}
	@Override
	public Object clothes() {
		return new Bag().ring();
	}
}
