package persons_p.personParts;

import clothes_p.clothesParts.ClotheGlasses;
import clothes_p.clothesParts.ClotheHead;

// 눈
public class Eyes {
	String name = "눈";
	ClotheGlasses cg; // 안경류
	
	public Eyes() {
		super();
	}

	public Eyes(ClotheHead ch, ClotheGlasses cg) {
		super();
		this.ch = ch;
		this.cg = cg;
	}

	@Override
	public String toString() {
		return "Head [name=" + name + ", ch=" + ch + ", cg=" + cg + "]";
	}
}
