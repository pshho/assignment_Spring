package classifications;

import menu.BeverageMenu;
import menu.HamburgerMenu;
import menu.SideMenu;

public class SellSet {
	String kind = "세트 메뉴";
	HamburgerMenu hb;
	SideMenu su;
	BeverageMenu bm;
	
	public SellSet(HamburgerMenu hb, SideMenu su, BeverageMenu bm) {
		this.hb = hb;
		this.su = su;
		this.bm = bm;
	}
	public void setHb(HamburgerMenu hb) {
		this.hb = hb;
	}
	public void setSu(SideMenu su) {
		this.su = su;
	}
	public void setBm(BeverageMenu bm) {
		this.bm = bm;
	}
}
