package clothing.personParts;

import org.springframework.stereotype.Component;

import clothing.clothesParts.ClotheHead;

@Component
public class Head {
	String name = "머리";
	ClotheHead ch; // 모자류
	
	public Head() {
		super();
	}
	public Head(String name, ClotheHead ch) {
		super();
		this.name = name;
		this.ch = ch;
	}
	@Override
	public String toString() {
		return "Head [name=" + name + ", ch=" + ch + "]";
	}
}
