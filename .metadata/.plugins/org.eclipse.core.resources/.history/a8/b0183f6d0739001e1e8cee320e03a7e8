	<bean name="table1" class="bar.store.Table" p:tableName="1번 테이블" />
	<bean name="table2" class="bar.store.Table">
		<constructor-arg value="2번 테이블" />
		<constructor-arg>
			<array>
				<map>
					<entry key-ref="alc2" value="4" />
				</map>
			</array>
		</constructor-arg>
		<constructor-arg>
			<array>
				<ref bean="alc2" />
				<ref bean="alc4" />
				<ref bean="alc6" />
			</array>
		</constructor-arg>
	</bean>
	
