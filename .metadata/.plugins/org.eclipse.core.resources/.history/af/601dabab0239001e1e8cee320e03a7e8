package bar.menu;

public class Alcohol {
	String kinds, name;
	int price;
	
	public void setKinds(String kinds) {
		this.kinds = kinds;
	}
	public void setName(String name) {
		this.name = name;
	}
	public void setPrice(int price) {
		this.price = price;
	}
	@Override
	public String toString() {
		return "Alcohol [kinds=" + kinds + ", name=" + name + ", price=" + price + "]";
	}
}
